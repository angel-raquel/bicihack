<section class="probootstrap-cover relative background-grey">
  <div class="container">
    <div class="row align-items-center text-center">
      <div class="col-md">
        <h1 class="border-bottom">The stations</h1>
      </div>
      <hr>
<table class="table">
  <thead>
    <tr>
      <th scope="col">id</th>
      <th scope="col">name</th>
      <th scope="col">light</th>
      <th scope="col">number</th>
      <th scope="col">activate</th>
      <th scope="col">no_available</th>
      <th scope="col">total_bases</th>
      <th scope="col">dock_bikes</th>
      <th scope="col">free_bases</th>
      <th scope="col">reservations_count</th>
    </tr>
  </thead>
  <tbody>
<% if(typeof stations != "undefined") { %>
<% stations.forEach(station => { %>
  <tr>
    <td><%= station.id %></td>
    <td><%= station.name %></td>
    <td><%= station.light %></td>
    <td><%= station.number %></td>
    <td><%= station.activate %></td>
    <td><%= station.no_available %></td>
    <td><%= station.total_bases %></td>
    <td><%= station.dock_bikes %></td>
    <td><%= station.free_bases %></td>
    <td><%= station.reservations_count %></td>
  </tr>
<% }) %>
<% } %>
  </tbody>
</table>
</div>
</div>
</section>
